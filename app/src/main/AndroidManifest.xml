<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.justinmeilinger.flashcardcalculator" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".showCardsActivityActivity"
            android:label="@string/title_activity_show_cards" >
        </activity>
        <activity
            android:name=".showCardsActivity"
            android:label="@string/title_activity_show_cards" >
        </activity>
        <activity
            android:name=".viewCardsActivity"
            android:label="@string/title_activity_view_cards" >
        </activity>
    </application>

</manifest>
